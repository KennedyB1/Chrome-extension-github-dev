{
    "name": "Github web editor",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Start web editor with a click",
    "action": {
        "default_title": "Github web editor"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}